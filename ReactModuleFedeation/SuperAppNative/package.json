{
  "name": "superappnative",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native webpack-start",
    "android": "react-native webpack-bundle --platform android --dev false && react-native run-android",
    "ios": "react-native webpack-bundle --platform ios --dev false && react-native run-ios",
    "web": "echo 'Use SuperApp for web' && exit 1",
    "clean": "lsof -ti :9001,:9002,:8081,:8082 2>/dev/null | xargs kill -9 2>/dev/null || true",
    "start:all": "npm run clean && echo 'Starting all services...' && ./start-all.sh"
  },
  "dependencies": {
    "@module-federation/enhanced": "^0.2.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "axios": "^1.6.0",
    "expo": "~54.0.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-safe-area-context": "4.14.0",
    "react-native-screens": "4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@callstack/repack": "^4.3.0",
    "@react-native-community/cli": "^15.0.0",
    "@react-native-community/cli-platform-android": "^15.0.0",
    "@react-native-community/cli-platform-ios": "^15.0.0",
    "@types/react": "~18.3.0",
    "babel-loader": "^9.1.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.9.3",
    "webpack": "^5.89.0"
  }
}

